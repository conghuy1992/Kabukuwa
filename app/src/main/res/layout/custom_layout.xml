<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tj="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/btnsell"
        android:layout_width="@dimen/btnsell_w"
        android:layout_height="@dimen/btnsell_h"
        android:layout_gravity="center">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:adjustViewBounds="true"
            android:src="@drawable/shop_sell_beetle_window" />

        <RelativeLayout
            android:id="@+id/setwh"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginBottom="10dp">

                <FrameLayout
                    android:id="@+id/bgcanh"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/bghorn2"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/bghorn"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/bgdau"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/bgbody"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/bgco"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/bgmat"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center"
                    android:visibility="gone" />

                <FrameLayout
                    android:id="@+id/canh"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />

                <FrameLayout
                    android:id="@+id/horn2"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />

                <FrameLayout
                    android:id="@+id/dau"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />

                <FrameLayout
                    android:id="@+id/body"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />

                <FrameLayout
                    android:id="@+id/co"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />

                <FrameLayout
                    android:id="@+id/mat"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />

                <FrameLayout
                    android:id="@+id/horn"
                    android:layout_width="@dimen/size_part"
                    android:layout_height="@dimen/size_part"
                    android:layout_gravity="center" />
            </FrameLayout>
        </RelativeLayout>

        <FrameLayout
            android:layout_width="140dp"
            android:layout_height="180dp"
            android:layout_gravity="center">

            <com.daydelight.kabukuwa.CustomTextView
                android:id="@+id/txtten"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|bottom"
                android:layout_marginBottom="35dp"
                android:text="ten"
                android:textColor="#000000"
                android:textSize="9dp"
                android:textStyle="bold"
                tj:textStroke="true"
                tj:textStrokeColor="#FFFFFF"
                tj:textStrokeWidth="3.5" />

            <TextView
                android:id="@+id/txthang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|bottom"
                android:layout_marginBottom="25dp"
                android:text="hang"
                android:textColor="#ffffff"
                android:textSize="9dp"
                android:textStyle="bold"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|bottom"
                android:layout_marginBottom="18dp">

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:src="@drawable/coin_icon" />

                <TextView
                    android:id="@+id/txtgiaban"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="gia"
                    android:textColor="#ffffff"
                    android:visibility="gone"
                    android:textSize="9dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/formatgiaban"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="gia"
                    android:textColor="#ffffff"
                    android:textSize="9dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <com.daydelight.kabukuwa.CustomTextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|top"
                android:layout_marginTop="30dp"
                android:text="Lv.1"
                android:textColor="#000000"
                android:textSize="8dp"
                android:textStyle="bold"
                tj:textStroke="true"
                tj:textStrokeColor="#FFFFFF"
                tj:textStrokeWidth="3.5" />
        </FrameLayout>

        <ImageView
            android:id="@+id/idloai"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:adjustViewBounds="true"
            android:src="@drawable/mypage_breeding_badge_kabu" />
    </FrameLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="10dp" />
</LinearLayout>